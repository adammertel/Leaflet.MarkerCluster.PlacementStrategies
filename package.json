{
  "name": "leaflet.markercluster.placementstrategies",
  "version": "0.1.1",
  "description": "leaflet.markercluster subplugin that defines more strategies to place clustered markers",
  "keywords": [
    "leaflet",
    "markercluster",
    "placement",
    "clustering"
  ],
  "author": "Adam Mertel, University of Vienna",
  "license": "ISC",
  "main": "dist/leaflet-markercluster.placementstrategies.js",
  "devDependencies": {
    "copyfiles": "^1.0.0",
    "eslint": "^4.2.0",
    "mkdirp": "latest",
    "npm-run-all": "^4.0.2",
    "onchange": "^2.5.0",
    "uglify-js": "^2.7.3"
  },
  "files": [
    "dist",
    "src",
    "demo"
  ],
  "scripts": {
    "create-dist-folder": "mkdirp dist",
    "eslint": "eslint src",
    "concat": "uglifyjs \"src/*.js\" -o dist/leaflet-markercluster.placementstrategies.src.js -b --preamble \" /* Adam Mertel | UNIVIE */ \" --wrap ",
    "minify": "uglifyjs dist/leaflet-markercluster.placementstrategies.src.js -o dist/leaflet-markercluster.placementstrategies.js -c -m --preamble \" /* Adam Mertel | UNIVIE */ \"",
    "onchange": "onchange src/**/*.js -- npm run compile",
    "compile": "npm run concat && npm run minify",
    "build": "run-s create-dist-folder eslint compile",
    "start": "run-p create-dist-folder onchange build"
  }
}
